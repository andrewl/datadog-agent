package {{.PkgName}}

import "testing"

var embedTests = []testing.InternalTest{
	{{- range .TestNames}}
	{
		Name: "{{.}}",
		F:    {{.}},
	},
	{{- end}}
}

func RunEmbedTests() {
	testing.Main(func(pat, str string) (bool, error) { return true, nil }, embedTests, nil, nil)
}
